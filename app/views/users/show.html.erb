<h1>User Details</h1>
<ul>
  <li><strong>Name: </strong><%= @user.name%></li>
  <li><strong>Email: </strong><%= @user.email%></li>
</ul>
<% @accounts.each do |account| %>
	<ul>
    <li><strong>Account No.: </strong><%= account.account_no %></li>
    <li><strong>Account Type.: </strong><%= account.account_type %></li>
    <li><strong>Available Balance.: </strong><%= account.balance %></li>
  </ul>
<% end %>